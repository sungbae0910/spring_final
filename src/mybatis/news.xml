<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="news">
	<!-- select -->
	
	<!-- insert -->
	<insert id='news_insert' parameterType="newController.NewsVo">
		insert into news_info values(seq_news_serial.nextvla, #{nTitle}, #{nContent}, #{nCompany}, sysdate, #{nRepoter}, #{nCategory}, 0, 0)
	</insert>



</mapper>	